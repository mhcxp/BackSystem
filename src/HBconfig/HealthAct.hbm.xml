<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping package="com.BS.domain">  
    <class name="HealthAct" table="heaact">   
        <id name="actId" column="actId">  
            <generator class="increment" />  
        </id>  
        <property name="actName" column="actName"/>
        <property name="txtPath" column="txtPath"/> 
        <property name="beginDate" column="beginDate"/> 
        <property name="phoAmount" column="phoAmount"/>  
        <property name="phoPath" column="phoPath"/>     
    </class>  
</hibernate-mapping>  