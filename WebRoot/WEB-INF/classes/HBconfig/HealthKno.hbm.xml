<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping package="com.BS.domain">  
    <class name="HealthKno" table="heakno">   
        <id name="knoId" column="knoId">  
            <generator class="increment" />  
        </id>  
        <property name="actName" column="actName"/>
        <property name="txtPath" column="txtPath"/>  
        <property name="phoAmount" column="phoAmount"/>  
        <property name="phoPath" column="phoPath"/>     
    </class>  
</hibernate-mapping>  