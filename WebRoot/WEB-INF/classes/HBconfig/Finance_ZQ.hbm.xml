<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping package="com.BS.domain">  
    <class name="Finance_ZQ" table="zqc">   
        <id name="zqId" column="zqId">  
            <generator class="increment" />  
        </id>  
        <property name="zqExp" column="zqExp"/>  
        <property name="comName" column="comName"/>  
        <property name="zqRate" column="zqRate"/> 
        <property name="buyTimes" column="buyTimes"/> 
        <property name="price" column="price"/>     
    </class>  
</hibernate-mapping>  