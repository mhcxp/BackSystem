<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping package="com.BS.domain">  
    <class name="Finance_HJ" table="hjc">   
        <id name="proId" column="proId">  
            <generator class="increment" />  
        </id>  
        <property name="proName" column="proName"/>
        <property name="proExp" column="proExp"/>  
        <property name="proIns" column="proIns"/>  
        <property name="proEva" column="proEva"/> 
        <property name="proCode" column="proCode"/> 
        <property name="proRate" column="proRate"/> 
        <property name="beginDate" column="beginDate"/> 
        <property name="buyTimes" column="buyTimes"/> 
        <property name="price" column="price"/>     
    </class>  
</hibernate-mapping>  