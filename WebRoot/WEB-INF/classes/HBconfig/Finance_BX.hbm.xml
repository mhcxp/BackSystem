<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
<hibernate-mapping package="com.BS.domain">  
    <class name="Finance_BX" table="bxc">   
        <id name="bxId" column="bxId">  
            <generator class="increment" />  
        </id>  
        <property name="comName" column="comName"/>
        <property name="bxExp" column="bxExp"/>  
        <property name="bxRate" column="bxRate"/>  
        <property name="buyTimes" column="buyTimes"/> 
        <property name="price" column="price"/>     
    </class>  
</hibernate-mapping>  